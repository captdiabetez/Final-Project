@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<p>List of Assets:</p>
@foreach (var b in Model.Assets)
{
    <p>@Html.DisplayFor(modelItem => b.DeviceName) @Html.DisplayFor(modelItem => b.DeviceType) @Html.DisplayFor(modelItem => b.Description)</p>
}


